<div id="documents">
  <% @document_groups.each do |group| %>
    <h3><a href="#"><%= group.first %></a></h3>
    <ul>
      <% group.last.each do |doc| %>
        <li><%= link_to doc.name, document_path(doc) %></li>
      <% end %>
    </ul>
  <% end %>
</div>
<script type="text/javascript">
  $(function() {
    $( '#documents' ).accordion({
      autoHeight: false,
//    collapsible: true,
      navigation: true
    });
  });
</script>
